GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FFmpeg" "${PODS_ROOT}/Headers/Public/FFmpegWrapper"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/FFmpegWrapper" $(inherited) "${PODS_ROOT}/FFmpeg/ffmpeg-ios-static-libs/lib"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/FFmpeg" -isystem "${PODS_ROOT}/Headers/Public/FFmpegWrapper"
OTHER_LDFLAGS = $(inherited) -ObjC -l"FFmpegWrapper" -l"avcodec" -l"avdevice" -l"avfilter" -l"avformat" -l"avutil" -l"bz2" -l"iconv" -l"swresample" -l"swscale" -l"z"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
